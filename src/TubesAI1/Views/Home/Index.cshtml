@{
    ViewData["Title"] = "Home Page";
    @using TubesAI1.Scheduling;
}

@{ 
    for (int z = 0; z < (int)ViewData["Length"]; z++)
    {

        <li>
        Nama Kelas: @ViewData["nama" + z.ToString()]<br />
        Nama Ruangan: @ViewData["ruangan" + z.ToString()]<br />
        Jam Mulai: @ViewData["jam" + z.ToString()]<br />
        Durasi: @ViewData["durasi" + z.ToString()]<br />
        Hari: @ViewData["hari" + z.ToString()]<br />
        </li>
    }
}
<p>@ViewData["Message"]</p>
<br /><br />
<form method="post" asp-action="Index" asp-controller="Home" enctype="multipart/form-data">
    <input type="file" name="files" multiple />
    <input type="submit" value="Upload" />
</form>